class Solution:
    def twoSum(self, nums:list, target: int):

        temp_dict = {}
        result_list = []

        for i in range(len(nums)):
            print(i)
            if nums[i] in temp_dict:

                result_list.extend([i,temp_dict[nums[i]]])
            else:
                temp_dict[target - nums[i]] = i

        return result_list
if __name__ == '__main__':
    solution = Solution()
    a = [6,8,4,2,1,3,4,5]
    b = 5
    result = solution.twoSum(a,b)
    print(result)

